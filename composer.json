{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "barryvdh/laravel-cors": "^0.11.3",
        "cmgmyr/messenger": "^2.17",
        "fideloper/proxy": "^4.1",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "laravel/framework": "5.8.*",
        "laravel/nova": "^2.0",
        "laravelcollective/html": "^5.8",
        "league/flysystem-aws-s3-v3": "^1.0",
        "pda/pheanstalk": "^4.0",
        "predis/predis": "^1.1@dev",
        "spatie/laravel-activitylog": "^3.4",
        "spatie/laravel-collection-macros": "^5.0",
        "spatie/laravel-permission": "^2.37",
        "spatie/laravel-view-models": "^1.1",
        "tymon/jwt-auth": "1.0.0-rc.4",
        "vinkla/hashids": "^6.0"
    },
    "require-dev": {
        "ahuggins/open-on-make": "^0.0.25",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "2.6.*",
        "beyondcode/laravel-dump-server": "^1.2",
        "beyondcode/laravel-er-diagram-generator": "^1.2",
        "beyondcode/laravel-query-detector": "^1.0",
        "beyondcode/laravel-view-xray": "^1.1",
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.8",
        "laravel/dusk": "^5.0",
        "laravel/telescope": "^2.0.0",
        "laravel/tinker": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\Cors\\ServiceProvider"
            ],
            "dont-discover": []
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix --allow-risky=yes"
        ],
        "pre-install-cmd": [
            "@rm ./vendor/laravel/nova"
        ],
        "pre-update-cmd": [
            "@rm ./vendor/laravel/nova"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "yarn install"
        ],
        "post-install-cmd": [
            "php artisan nova:publish",
            "yarn install",
            "npm run dev",
            "php artisan config:clear",
            "php artisan cache:clear",
            "php artisan view:clear",
            "php artisan route:clear"
        ],
        "post-update-cmd": [
            "php artisan nova:publish",
            "npm run dev",
            "php artisan config:clear",
            "php artisan cache:clear",
            "php artisan view:clear",
            "php artisan route:clear"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models -N",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}